# Directory to read outputs from (read-only)
COMFY_OUTPUT_DIR=/var/lib/comfyui/output

# Working directory where favorites/moves/folders are stored
DATA_DIR=~/comfy_viewer/data

# Server port (production)
PORT=8008

# Auto-sync interval (milliseconds). 0 disables.
SYNC_INTERVAL_MS=0

# Thumbnail settings
THUMB_MAX=512
THUMB_QUALITY=72
