{
  "name": "comfy-output-viewer",
  "version": "0.9.23",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"node --watch src/server/index.js\" \"vite\"",
    "mock:seed": "node scripts/seed-mock-dev-data.mjs",
    "dev:mock": "npm run mock:seed && MOCK_DEV_MODE=1 COMFY_OUTPUT_DIR=.mock-dev/source DATA_DIR=.mock-dev/data concurrently -k \"node --watch src/server/index.js\" \"vite\"",
    "install:unix": "bash scripts/install-unix.sh",
    "install:linux": "bash scripts/install-linux.sh",
    "install:macos": "bash scripts/install-macos.sh",
    "install:windows": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/install-windows.ps1",
    "build": "vite build",
    "preview": "vite preview --port 8008",
    "start": "node scripts/start-production.mjs",
    "test": "npm run test:server && npm run test:client",
    "test:all": "npm run test",
    "test:server": "vitest run --config vitest.server.config.ts",
    "test:client": "vitest run --config vitest.client.config.ts",
    "test:server:watch": "vitest --config vitest.server.config.ts",
    "test:client:watch": "vitest --config vitest.client.config.ts",
    "test:coverage": "npm run test:server:coverage && npm run test:client:coverage",
    "test:server:coverage": "vitest run --config vitest.server.config.ts --coverage --coverage.reportsDirectory=coverage/server",
    "test:client:coverage": "vitest run --config vitest.client.config.ts --coverage --coverage.reportsDirectory=coverage/client"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@saintno/comfyui-sdk": "^0.2.49",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lucide-react": "^0.563.0",
    "radix-ui": "^1.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "react-zoom-pan-pinch": "^3.6.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^8.2.2",
    "jsdom": "^28.0.0",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^4.0.18"
  }
}
